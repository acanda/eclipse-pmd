<tests>
  <pmdReferenceId>category/java/codestyle.xml/UnnecessaryReturn</pmdReferenceId>
  <language>java 1.8</language>

  <test name="UnnecessaryReturn">
    <setup>
      <source>
public class UnnecessaryReturn {
    public void foo() {
        <marker>return;</marker>
    }
}
      </source>
    </setup>
    <expected>
      <source>
public class UnnecessaryReturn {
    public void foo() {
    }
}
      </source>
      <image>QUICKFIX_REMOVE</image>
      <label>Remove return</label>
      <description>Removes the return statement.</description>
    </expected>
  </test>

</tests>